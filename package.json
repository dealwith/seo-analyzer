    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "ts-node": "^10.9.2",
